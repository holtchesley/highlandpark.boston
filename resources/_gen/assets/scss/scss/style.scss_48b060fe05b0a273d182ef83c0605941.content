*{font-family:roboto,sans-serif}body{margin:0;padding:0}.homepage-content{position:absolute;margin:0;padding:10%;height:100%;width:100%;overflow:hidden}@media(max-aspect-ratio:4/5){.homepage-content{background-image:url(/images/portrait/cochituate_background.png);background-position:bottom 50px right 40px;background-repeat:no-repeat;background-size:100vw}}@media(min-aspect-ratio:4/5){.homepage-content{background-image:url(/images/square/public_meeting_house.png);background-position:50%;background-size:cover}}@media(min-aspect-ratio:5/4){.homepage-content{background-image:url(/images/landscape/fort_hill_jazz.png);background-position:50%;background-size:cover}}.homepage-content .homepage-location h1,.homepage-content .homepage-location h2{margin:0;padding:0}.homepage-content .menu{list-style:none}.homepage-content .menu li a{color:#000}#map{width:100vw;height:95vh}header .menu{list-style:none;display:grid;grid-template-columns:1fr 1fr 1fr 1fr;width:100%;height:2vh;padding:1rem 0 1.25rem;margin:0;justify-items:center;border-bottom:1px solid #000}header .menu li a{color:#000}header .menu li a:visited{color:#000}main>div{width:95vw;margin:0 auto}#calendar .event-popup div+div{margin-top:10px}#census{padding:2rem}#census .question-prompt{font-size:large;font-weight:700}#census label{display:block;margin:.25rem}#census form div+div{margin-top:2rem}#census div.inset{margin-left:1rem}#census form input[type=text]{width:100%}#census form input[type=checkbox]{margin-right:.5rem}#census form input[type=radio]{margin-right:.5rem}#census form textarea{width:100%}#census form input[type=submit]{width:100%;height:2rem;font-size:large}#about{margin:2rem}